<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval" >
            <solid android:color="#ff0000" />
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval" android:innerRadius="16dp">
            <stroke
                android:width="1dp"
                android:color="#78d9ff" />
            <size
                android:width="32dp"
                android:height="32dp" />
        </shape>
    </item>


</layer-list>